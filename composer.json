{
    "name": "myapp/cakephp5",
    "description": "CakePHP 5 application",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "cakephp/app": "5.*",
        "cakephp/core": "5.*",
        "cakephp/orm": "5.*",
        "cakephp/database": "5.*",
        "cakephp/migrations": "^5.0",
        "twbs/bootstrap": "^5.0",
        "ext-pgsql": "*"
    },
    "require-dev": {
        "cakephp/debug_kit": "^5.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "App\\Console\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "App\\Console\\Installer::postUpdate"
        ]
    }
}
